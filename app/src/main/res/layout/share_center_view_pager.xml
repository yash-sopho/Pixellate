        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@color/newcolor">
            <!--Main Text-->


            <ImageView
                android:id="@+id/dev_photo"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="36dp"
                android:layout_marginTop="50dp"
                app:srcCompat="@drawable/dev" />

            <TextView
                android:id="@+id/the_iniqitous_scorpion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/dev_photo"
                android:fontFamily="sans-serif-smallcaps"
                android:text="the_iniquitous_scorpion"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/dev_photo"
                android:layout_alignStart="@+id/the_iniqitous_scorpion"
                android:layout_marginBottom="21dp"
                android:layout_marginStart="60dp"
                android:fontFamily="casual"
                android:text="Yash Devikar"
                android:textSize="12dp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/fb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/facebook"
                android:layout_below="@+id/github"
                android:layout_alignStart="@+id/insta" />

            <ImageButton
                android:id="@+id/mail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/mail"
                android:layout_below="@+id/github"
                android:layout_toStartOf="@+id/fb" />

            <ImageButton
                android:id="@+id/github"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                app:srcCompat="@drawable/github"
                android:layout_below="@+id/textView2"
                android:layout_alignStart="@+id/mail" />

            <ImageButton
                android:id="@+id/insta"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/insta"
                android:layout_below="@+id/textView2"
                android:layout_alignEnd="@+id/textView" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/dev_photo"
                android:layout_marginTop="61dp"
                android:fontFamily="casual"
                android:gravity="center"
                android:text="For InApp Suggestions or Queries, Follow me or Drop a mail.
For developers, Send a Pull Request at the Github Repository and Contribute in building the app.."
                android:textStyle="normal" />

            <!--Main Text-->
        </RelativeLayout>


    </RelativeLayout>

